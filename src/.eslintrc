{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "linebreak-style": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "object-curly-spacing": ["warn", "always"],
    "quotes": ["warn", "single"],
    "indent": ["warn", 2, { "SwitchCase": 1 }]
  },
  "extends": [
    "eslint:recommended", "google"
  ]
}
